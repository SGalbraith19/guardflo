# ---- Environment ----
ENVIRONMENT=production

# ---- Database ----
DATABASE_URL=postgresql://user:password@host:5432/dbname

# ---- Webhooks ----
ENABLE_WEBHOOKS=true
WEBHOOK_TIMEOUT_SECONDS=5

# ---- Capacity / Safety ----
ENABLE_CAPACITY_ENFORCEMENT=true
